    <script type="text/javascript">
        $(function(){
            $(document).on('change', '#selectproduct', function () {
                var slectedValue = $(this).val();
                if (slectedValue) {
                    $.ajax({
                        url: '@Url.Action("GetSubProductModel", "Supervisor")',
                        type: 'GET',
                        data: { productId: slectedValue },
                        success: function (data) {
                            debugger;
                            var subCategoryDropdown = $('#selectmodel');
                            subCategoryDropdown.empty();
                            subCategoryDropdown.append('<option value="">--Select Subcategory--</option>');
                            $.each(data, function (index, subCategory) {
                                subCategoryDropdown.append('<option value="' + subCategory.productId + '">' + subCategory.modelName + '</option>');
                            });
                        }
                    });
                } else {
                    $('#selectmodel').empty().append('<option value="">--Select Subcategory--</option>');
                }
                console.log(slectedValue);
            });

            //validations
            $(document).on('change', '.resource-allocation-block .form-control', function () {
                if ($(this).val().trim() === "") {
                    isValid = false;
                    $(this).parents(".form-group").addClass("has-error");
                }else {
                        $(this).parents(".form-group").removeClass("has-error");
                    }
            });


            //Wizard
            let currentStep = 0;
            const steps = $(".step");
            const indicators = $(".step-indicator");

            function updateSteps() {
                steps.removeClass("active").eq(currentStep).addClass("active");
                indicators.each(function (index) {
                    $(this).removeClass("active completed");
                    if (index < currentStep) {
                        $(this).addClass("completed");
                    } else if (index === currentStep) {
                        $(this).addClass("active");
                    }
                });
            }

            $(".next-step").click(function () {
                if (currentStep < steps.length - 1) {
                    currentStep++;
                    updateSteps();
                }
            });

            $(".prev-step").click(function () {
                if (currentStep > 0) {
                    currentStep--;
                    updateSteps();
                }
            });

            $("#submitForm").click(function () {
                // alert("Form submitted successfully!");
                location.href='production-detail.html'
                $("#multiStepForm")[0].reset(); // Reset the form
                currentStep = 0;
                updateSteps();
            });

            updateSteps(); // Initialize steps and progress bar
        });

